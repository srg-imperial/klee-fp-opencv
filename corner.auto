#!/bin/sh

. ./Makefile.config

NAME=$1
shift

OUTDIR=corner.auto.out.$NAME
rm -rf $OUTDIR
mkdir $OUTDIR

for algo in eigenval harris ; do
	id=$algo
	echo $id
	$KLEE_BUILD_PATH/bin/klee -libc=uclibc -output-source=false -output-istats=false -output-dir=$OUTDIR/klee-out-$id -instrument-simd ./$algo.opt.bc "$@" > $OUTDIR/$id.log 2>&1
done
